<dynamic_object>
	<diesel materials="units/pd2_mod_custom/cus_explosive_propane_tank/cus_explosive_propane_tank" orientation_object="rp_root" />
	<sequence_manager file="units/pd2_mod_custom/cus_explosive_propane_tank/cus_explosive_propane_tank" />
	
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="rp_root" />
			<object name="c_c" collision_type="convex" padding="-2.5" />
		</body>
		<body name="body_dynamic" enabled="false" template="dynamic">
			<object name="rp_root" />
			<object name="c_c" collision_type="convex" padding="-2.5" mass="60" />
		</body>
	</bodies>
	
	<decal_surfaces default_material="metal_catwalk">
		<decal_mesh name="c_c" enabled="true" material="metal_catwalk" />
	</decal_surfaces>
	
	<graphics>
		<graphic_group name="grp_all" enabled="true" >
			<object name="g_g" enabled="true" />
			<object name="c_c" enabled="true" shadow_caster="true" />
		</graphic_group>
	</graphics>
</dynamic_object>
